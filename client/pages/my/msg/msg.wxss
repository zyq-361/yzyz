/* pages/msg/msg.wxss */
/* 功能列表样式 */
.weui_cell{
    position: relative;
    display: flex;
    padding: 5.1%;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    /* border-bottom: 1px solid #dadada; */
    border-top:  1px solid #dadada; 
}
.weui_cell_hd{
  display: inline-block;
  width: 25px;
  margin-right: 5px;
}
.weui_cell_hd image{
  width: 100%;
  height: 20px;
  vertical-align: -2px;
}
.weui_cell_bd{
  display: inline-block;
}
.weui_cell_bd_p{
  font-size: 14px;
  color: #939393;
}