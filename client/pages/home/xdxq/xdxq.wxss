page{
  background-color: #f2f2f2;
}
.container{
  display: flex;
  flex-direction: column;
  align-items: center;
}

swiper,swiper image{
  width:100%;
  height: 400rpx;
}



.dyCard{
  background: #fffcfc;
  border-radius: 40rpx;
  margin-top: -5rpx;
  width: 100%;
  height: 375rpx;
}

.above{
  width: 100%;
  height: 250rpx;
}

.above .left{
  float: left;
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  margin-top: 5%;
  margin-left: 5%;
}

.detail{
  width: 60%;
  float: right;
  margin-right: 5%;
}
.top{
  height: 70rpx;
  /* width: 60%; */
  /* border: 1px solid black; */
  margin-top: 5%;
  margin-right: 5%;
  font-family: 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;
  font-size: 40rpx;
  font-weight: 500;
}
.top image{
  width: 35rpx;
  height: 35rpx;
}

.mid{
  /* width: 60%; */
  height: 70rpx;
}

.bottom{
  float: left;
  height: 60rpx;
}
.line{
  border-top:  1px solid #dadada; 
}

.bellow{
  height: 150rpx;
  display: flex;
  margin-top: 5%;
  margin-left: 5%;
}
.bellow image{
  width: 50rpx;
  height: 50rpx;
}

.fwCards{
  width: 100%;
  text-align: center;
  background-color: #fffcfc;
}

.fwCard{
  background-color: #fffcfc;
  width: 100%;
  height: 250rpx;
  margin-top: 5%;
  margin-left: 5%;
}

.fwCard image{
  float: left;
  width: 200rpx;
  height: 200rpx;
  margin-bottom: 5%;
}

.fwCard .right{
  width: 60%;
  height: 200rpx;
  font-size: 36rpx;
  float:right;
  margin-top: 5%;
  margin-right: 5%;
}

.fwCard .line{
  display: flex;
  width: 90%;
  height: 10rpx;
  border-top:  1px solid #dadada; 
}

.consult{
  position: fixed;
  bottom: 0;
  width: 100%;
}

.consult button{
  color: white;
  /* float: right; */
  font-size: 35rpx;
  margin: 20rpx 20rpx;
}